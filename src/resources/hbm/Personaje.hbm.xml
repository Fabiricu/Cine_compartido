<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="poo.cine.Personaje" table="Personaje">
          <id name="id" column="id_personaje" type="int">
          <generator class="increment" />
      </id>
      <property name="nombreEnPelicula"/>
      <many-to-one name="Actor" class="poo.cine.Actor" column="id_actor"></many-to-one>
      
  </class>
</hibernate-mapping>
